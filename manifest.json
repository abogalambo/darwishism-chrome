{
  // Required
  "manifest_version": 2,
  "name": "Darwishism Reader",
  "version": "1.0",

  // Recommended
  "description": "Replaces the chrome new tab with an image from darwishism.tumblr.com/",

  // Optional
  "author": "abogalambo",
  "chrome_url_overrides": {
    "newtab": "new_tab.html"
  },
  
  "homepage_url": "https://darwishism.tumblr.com/",
  "incognito": "not_allowed",
  "offline_enabled": false
}